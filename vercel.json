{
    "functions": {
        "api/chat.php": {
            "runtime": "vercel-php@0.5.2",
            "excludeFiles": "{test/**}",
            "memory": 256,
            "maxDuration": 60
        },
        "api/models.php": {
            "runtime": "vercel-php@0.5.2",
            "excludeFiles": "{test/**}",
            "memory": 256,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/chat",
            "dest": "/api/chat"
        },
        {
            "src": "/models",
            "dest": "/api/models"
        }
    ]
}